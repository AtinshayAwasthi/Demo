//minimum value
#include<iostream>
#include<climits>
using namespace std;
int main()
{int n;
    cin>>n;
    int arr[n];
    cout<<"enter the elements:"<<endl;
    for(int i=0;i<n;i++)
    {cin>>arr[i];}
    int t=INT_MAX;
    for(int i=0;i<n;i++){if(t>arr[i]){
            t=arr[i];}}
    cout<<"the minimum element is : "<< t;
}
