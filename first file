//minimum value
#include<iostream>
#include<climits>
using namespace std;
int main()
{int x;
    cin>>x;
    int a[x];
    cout<<"enter the elements:"<<endl;
    for(int i=0;i<x;i++)
    {cin>>a[i];}
    int t=INT_MAX;
    for(int i=0;i<x;i++){if(t>a[i]){
            t=a[i];}}
    cout<<"the minimum element is : "<< t;
}
