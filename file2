// product of all elements
#include<iostream>
using namespace std;
int main()
{
    int a[5] , m=1;
    cout<<"enter the elements:"<<endl;
    for(int i=0 ;i<5;i++)
    {
        cin>>a[i];
    }
    cout<<endl;
    cout<<"product of all elements are :" ;
    for(int i=0;i<5;i++)
    {
        m=m*a[i];
    }
    cout<<m;
}
