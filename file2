// product of all elements
#include<iostream>
using namespace std;
int main()
{
    int arr[5] , mult=1;
    cout<<"enter the elements:"<<endl;
    for(int i=0 ;i<5;i++)
    {
        cin>>arr[i];
    }
    cout<<endl;
    cout<<"product of all elements are :" ;
    for(int i=0;i<5;i++)
    {
        mult=mult*arr[i];
    }
    cout<<mult;
}
